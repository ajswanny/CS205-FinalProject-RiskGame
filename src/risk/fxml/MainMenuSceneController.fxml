<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="root" prefHeight="500" prefWidth="800.0" stylesheets="@../css/MainMenuScene.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="risk.controller.MainMenuSceneController">
   <children>
      <VBox alignment="CENTER" layoutX="40.0" layoutY="130.0" prefHeight="240.0" prefWidth="208.0" spacing="10.0" stylesheets="@../css/SceneButtons.css" AnchorPane.bottomAnchor="130.0" AnchorPane.topAnchor="130.0">
         <children>
            <Button fx:id="playGame" mnemonicParsing="false" prefHeight="48.0" prefWidth="89.0" stylesheets="@../css/SceneButtons.css" text="Play">
               <font>
                  <Font name="Helvetica" size="15.0" />
               </font></Button>
            <Button fx:id="quitGame" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="46.0" prefWidth="80.0" text="Quit" />
            <Button fx:id="aboutGame" alignment="CENTER" mnemonicParsing="false" prefHeight="46.0" prefWidth="80.0" text="About" />
         </children>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </VBox>
   </children>
</AnchorPane>
